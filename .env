# ibooky - Configurazione Backend (Development)

# Server
NODE_ENV=development
PORT=3000
API_VERSION=v1

# Supabase (da configurare con le tue credenziali)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# JWT
JWT_SECRET=ibooky-super-secret-jwt-key-change-this-in-production-2024
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:5173,http://localhost:8080

# Email (SMTP predefinito)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_FROM_EMAIL=noreply@ibooky.com
SMTP_FROM_NAME=ibooky

# Timezone
DEFAULT_TIMEZONE=Europe/Rome

# Logging
LOG_LEVEL=info
